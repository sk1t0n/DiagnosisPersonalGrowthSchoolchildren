<Window x:Class="DiagnosisPersonalGrowthSchoolchildren.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiagnosisPersonalGrowthSchoolchildren"
        mc:Ignorable="d"
        Title="Диагностика личностного роста школьников" Height="210" Width="450">
    <StackPanel>
        <Label Margin="25, 10, 25, 10">Название листа в Excel файле</Label>
        <TextBox Margin="30, 0, 30, 0" x:Name="TextSheetName" TextChanged="TextChangedEventHandler"></TextBox>
        <Button IsEnabled="False" Width="250" Height="40" Margin="0, 30, 0, 0" x:Name="ButtonOpenDialog" Click="ButtonOpenDialog_Click">Открыть Excel файл и посчитать результат</Button>
    </StackPanel>
</Window>
